{
  "model": "Vasicek",
  "model_name": "Vasicek Interest Rate Model",
  "currency_pair": "EUR/USD",
  "parameters": {
    "initial_rate": 1.10,
    "mean_reversion": 1.5,
    "long_term_mean": 1.10,
    "volatility": 0.15,
    "risk_free_rate": 0.02
  },
  "simulation": {
    "num_simulations": 50000,
    "num_steps": 180,
    "dt": 1.0/252.0,
    "time_horizon_days": 180,
    "batch_size": 2000,
    "use_batch_processing": true
  },
  "random": {
    "generator": "normal",
    "seed": 54321,
    "mean": 0.0,
    "stddev": 1.0
  },
  "output": {
    "save_paths": true,
    "max_paths_to_save": 100,
    "save_final_prices": true,
    "save_summary": true,
    "format": "csv",
    "prefix": "eur_usd_vasicek",
    "directory": "results"
  },
  "analysis": {
    "confidence_levels": [0.95, 0.99],
    "risk_analysis": true,
    "stress_test": true,
    "scenario_analysis": true
  }
}
